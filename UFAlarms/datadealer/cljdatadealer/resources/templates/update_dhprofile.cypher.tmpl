MATCH (p:DHProfile {dhprofileid: "{{dhprofileid}}"})
SET p += { props }
RETURN {
    dhprofileid: p.dhprofileid,
    name: p.name,
    description: p.description,
    setPoint: p.setPoint,
    gain: p.gain,
    resetTime: p.resetTime,
    minDrive: p.minDrive,
    maxDrive: p.maxDrive,
    slewRate: p.slewRate
} AS dhprofile
